{% extends "layouts/manage-prototype.njk" %}

{% set pageName = "Session cleared" %}

{% block content %}

  <h1>
    Session cleared
  </h1>

  <p>
    The session data has been cleared.
  </p>

  <p>
    {% if returnUrl %}
