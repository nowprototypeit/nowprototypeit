{% extends "layouts/manage-prototype.njk" %}

{% block pageScripts %}
  <noscript>
    <meta id="remove-on-auto-reload-enable" http-equiv="refresh" content="5"/>
  </noscript>
  {% if lastReloadTimestamp %}
    <script>
    window.nowPrototypeItGOVUK = window.nowPrototypeIt = window.nowPrototypeItGOVUK || window.nowPrototypeIt || {}
    window.nowPrototypeIt.lastReload={{lastReloadTimestamp}}
    </script>
  {% endif %}
  <script src="/manage-prototype/assets/scripts/reloader-client.js"></script>
{% endblock %}
