{% macro govukHeader() %}<p>Macro not available 'govukHeader'</p>{% endmacro %}
{% macro govukFooter() %}<p>Macro not available 'govukFooter'</p>{% endmacro %}
{% macro govukBreadcrumbs() %}<p>Macro not available 'govukBreadcrumbs'</p>{% endmacro %}
{% macro govukButton() %}<p>Macro not available 'govukButton'</p>{% endmacro %}
{% macro govukBackLink() %}<p>Macro not available 'govukBackLink'</p>{% endmacro %}
{% macro govukSummaryList() %}<p>Macro not available 'govukSummaryList'</p>{% endmacro %}
{% macro govukPanel() %}<p>Macro not available 'govukPanel'</p>{% endmacro %}
{% macro govukPagination() %}<p>Macro not available 'govukPagination'</p>{% endmacro %}
{% macro govukTaskList() %}<p>Macro not available 'govukTaskList'</p>{% endmacro %}

<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>{% block pageTitle %}{% endblock %}</title>
    {% block headIcons %}{% endblock %}

    {% block head %}{% endblock %}
  </head>
  <body>
    <h4>It looks like GOV.UK Frontend isn't installed</h4>
    <p>To install it visit <a href="/manage-prototype/plugin/npm:govuk-frontend">/manage-prototype/plugin/npm:govuk-frontend</a></p>
    <p>You might want to check <strong>app/views/layouts/main.njk</strong> to see if you're using a govuk branded layout there</p>
    <hr/>
    {% block bodyStart %}{% endblock %}

    {% block skipLink %}{% endblock %}

    {% block header %}
      {{ govukHeader() }}
    {% endblock %}

    {% block main %}
      {% block beforeContent %}{% endblock %}
      <main>
        {% block content %}{% endblock %}
      </main>
    {% endblock %}

    {% block footer %}
    {% endblock %}

    {% block bodyEnd %}{% endblock %}
  </body>
</html>